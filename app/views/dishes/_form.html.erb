<%= form_with(model: [current_user.establishment, @dish]) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :calories %>
    <%= f.text_area :calories %>
  </div>
  <%= f.submit 'Salvar' %>
<% end %>