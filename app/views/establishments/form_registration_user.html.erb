<%= form_with(url: user_new_establishment_path(@establishment.id) , class: 'form-edit') do |f| %>
  <div class='mb-3 mt-5'>
    <%= f.label :email, 'E-mail', class: 'form-label' %>
    <%= f.text_field :email, class: 'form-control', placeholder: 'ex: Café da manhã' %>
  </div>
  <div class='mb-3 mt-5'>
    <%= f.label :cpf, 'CPF', class: 'form-label' %>
    <%= f.text_field :cpf, class: 'form-control'%>
  </div>
  <%= f.submit 'Salvar', class: 'btn btn-primary'%>
<% end %>